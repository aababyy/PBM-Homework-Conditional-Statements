        {
            if (aa > cc)
            {
                Console.WriteLine(aa);
            }
            else
            {
                Console.WriteLine(cc);
            }
        }
        else
        {
            if (bb > cc)
            {
                Console.WriteLine(bb);
            }
            else
            {
                Console.WriteLine(cc);
            }
        }
    */
    }
}
